{
  "name": "contract-scan",
  "version": "1.0.0",
  "description": "Cross-chain smart contract search",
  "author": {
    "name": "Timur Badretdinov",
    "url": "https://github.com/Destiner"
  },
  "keywords": [
    "evm",
    "ethereum",
    "smart-contracts",
    "devtools"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/scope-sh/contract-scan.git"
  },
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "cache": "bun scripts/cache.ts",
    "lint": "bun run lint:prettier && bun run lint:eslint && bun run lint:stylelint",
    "lint:prettier": "prettier -c \"{api,components,data,pages,public,scripts,src,utils}/**/*.{json,js,ts,vue}\"",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint \"{api,components,data,pages,public,scripts,src,utils}/**/*.{css,vue}\"",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit"
  },
  "dependencies": {
    "@fontsource-variable/inconsolata": "^5.0.19",
    "@fontsource-variable/inter": "^5.0.20",
    "@types/react": "^18.3.5",
    "@vercel/node": "^3.2.12",
    "@vercel/og": "^0.6.2",
    "@vueuse/core": "^11.0.3",
    "nuxt": "^3.13.1",
    "react": "^18.3.1",
    "viem": "^2.21.3",
    "vue": "3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.6",
    "@types/bun": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/compiler-sfc": "^3.5.3",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vue-tsc": "^2.1.6"
  }
}
